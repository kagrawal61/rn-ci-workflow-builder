{
  "kind": "static-analysis",
  "options": {
    "name": "React Native Static Analysis",
    "triggers": {
      "push": {
        "branches": ["main"],
        "ignorePaths": ["docs/**", "*.md"]
      },
      "pullRequest": {
        "branches": ["main"]
      },
      "workflowDispatch": true
    },
    "nodeVersions": [18],
    "packageManager": "yarn",
    "runsOn": "ubuntu-latest"
  }
}
