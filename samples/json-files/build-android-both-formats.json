{
  "kind": "build",
  "options": {
    "name": "React Native Android Both Formats Build",
    "nodeVersions": [20],
    "packageManager": "yarn",
    "triggers": {
      "push": {
        "branches": ["main", "release/**"]
      },
      "pullRequest": {
        "branches": ["main"]
      },
      "workflowDispatch": true
    },
    "env": {
      "NODE_ENV": "production"
    },
    "cache": {
      "enabled": true
    },
    "build": {
      "platform": "android",
      "variant": "release",
      "storage": "github",
      "notification": "both",
      "includeHealthCheck": true,
      "androidOutputType": "both"
    }
  }
}