{
  "kind": "health-check",
  "options": {
    "name": "Test Health Check with Skip Conditions",
    "triggers": {
      "push": {
        "branches": ["main", "develop"]
      },
      "pullRequest": {
        "branches": ["main", "develop"]
      }
    },
    "skip": {
      "commitMessageContains": "[skip ci]",
      "prTitleContains": "[skip checks]"
    },
    "packageManager": "yarn",
    "nodeVersions": [20],
    "runsOn": "ubuntu-latest"
  }
}